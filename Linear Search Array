#include<iostream>
using namespace std;

void inputarr(int arr[], int num){
	for(int i=0;i<num;i++){
		cin>>arr[i];
	}
	
}

void outarr(int arr[], int num){
	for(int i=0;i<num;i++){
		cout<<arr[i]<<" ";
	}
	cout<<endl;
}

void searcharr(int arr[], int num, int key){
	for(int i=0; i<num; i++){
		if(arr[i]==key){
			cout<<"Element found";
		}
	}
	
	cout<<"element not present in array!!!!!!!!!!!!!!!";
}

int main(){
    
    int num;
    int arr[num];
    int key;
    
    cout<<"Enter the size of array. : ";
    cin>>num;
    
    inputarr(arr, num);
    outarr(arr, num);
    
    cout<<"Enter key for search : ";
    cin>>key;
    
    searcharr(arr, num, key);
    
    return 0;
}
